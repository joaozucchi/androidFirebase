<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LogadoActivity">

    <Button
        android:id="@+id/buttonLogoff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="119dp"
        android:text="buttonLogoff"
        android:onClick="deslogar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/emailCurrent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="74dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/buttonLogoff"
        app:layout_constraintStart_toStartOf="@+id/buttonLogoff" />
</androidx.constraintlayout.widget.ConstraintLayout>